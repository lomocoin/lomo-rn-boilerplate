{
  "editor.formatOnSave": true,
  "javascript.format.enable": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[java]": {
    "editor.formatOnSave": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/_docs": true,
    "**/ios/ThirdLibs": true,
    "**/ios/Pods": true,
  },
  "cSpell.words": [
    "appcenter",
    "appversion",
    "axiosv",
    "cloud",
    "codepush",
    "commitlint",
    "datetime",
    "devmenu",
    "devtools",
    "dropdown",
    "ellipsize",
    "fullwidth",
    "gitrev",
    "icomoon",
    "inspectable",
    "iphone",
    "iphonex",
    "livelyness",
    "lomo",
    "lomocoin",
    "mailto",
    "mobx",
    "nowrap",
    "pageable",
    "postbump",
    "postversion",
    "precommit",
    "reactotron",
    "screencap",
    "scrollable",
    "sdcard",
    "setenv",
    "signin",
    "signout",
    "signup",
    "simctl",
    "submodule",
    "tabbar",
    "todos",
    "typecheck",
    "typedoc",
    "unmount",
    "xcrun"
  ],
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "java.import.maven.enabled": false,
  "java.autobuild.enabled": false,
}
